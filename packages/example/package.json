{
  "name": "conduit-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example workspace showing Conduit DI compiler usage",
  "scripts": {
    "build": "node ../conduit/lib/cli.js compile",
    "build:dry": "node ../conduit/lib/cli.js compile --dry-run",
    "list": "node ../conduit/lib/cli.js list",
    "analyze:user": "node ../conduit/lib/cli.js analyze userService",
    "analyze:notification": "node ../conduit/lib/cli.js analyze notificationService",
    "test": "tsx test-workspace.ts"
  },
  "dependencies": {
    "conduit": "file:../conduit"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsx": "^4.19.1"
  }
}
