{
  "name": "conduit-example",
  "version": "0.1.0-alpha.1",
  "description": "Example application demonstrating @conduit/di and @conduit/cli integration",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run generate && tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "generate": "conduit compile",
    "generate:dev": "npm run generate && npm run dev",
    "clean": "rimraf lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "@conduit/cli": "*",
    "@conduit/di": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "jest": "^29.5.0",
    "prettier": "^3.6.0",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "conduit",
    "dependency-injection",
    "example",
    "typescript"
  ],
  "author": "Joseph Lyons",
  "license": "MIT"
}
