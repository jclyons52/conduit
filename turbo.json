{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "src/generated/**", "generated/**", "dist/**", ".vitepress/dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "compile": {
      "dependsOn": ["^build"],
      "outputs": ["generated/**"]
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}